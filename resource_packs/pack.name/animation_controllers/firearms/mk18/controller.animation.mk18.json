// Coded by @JustAsh0005 for ActualGuns3D, you aren't allowed to Edit, alter, modify, adapt, translate or otherwise change the whole code. 

{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.mk18.first_person": {
			"initial_state": "draw",
			"states" : {
				"draw": {
					"animations": [
						{
							"mk18.first_person.draw": "v.mk18 && v.is_first_person"
						}
					],
					"transitions": [
						{
							"first_person": "q.all_animations_finished && v.mk18 && v.is_first_person"
						}
					]
				},
				"first_person" : {
					"animations" : [
						{ 
						    "mk18.first_person.holding": "v.is_first_person && v.mk18"
						}
					],
					"transitions" : [
						{
							"aim": "v.is_first_person && v.mk18 && v.s"
						},
						{
							"sprinting": "v.is_first_person && v.mk18 && v.m && !v.r"
						},
						{
							"reloading": "v.is_first_person && v.is_holding_mk18empty && v.r"
						},
						{
							"firing": "v.is_first_person && v.is_holding_mk18 && v.u"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"firing": {
					"animations": [
						{
							"mk18.first_person.fire": "v.is_first_person && v.is_holding_mk18"
						}
					],
					"transitions": [
						{
							"first_person": "v.is_holding_mk18empty && v.is_first_person"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						},
						{
							"first_person": "v.is_first_person && v.mk18 && v.s"
						},
						{
							"firing.delay": "v.is_first_person && v.is_holding_mk18 && q.all_animations_finished"
						}
					]
				},
				"firing.delay": {
					"on_entry": [ "v.t = q.life_time;" ],
					"animations": [
						{
							"mk18.first_person.holding": "v.is_first_person && v.mk18"
						}
					],
					"transitions": [
						{
							"firing": "(q.life_time - v.t) >= 0.01 && v.is_holding_mk18 && v.u"
						},
						{
							"first_person": "v.is_first_person && (!v.is_holding_mk18 || v.is_holding_mk18empty) || !v.u"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						}
					]
				},
				"firing.aimed": {
					"animations": [
						{
							"mk18.first_person.aim_fire": "v.is_first_person && v.is_holding_mk18"
						}
					],
					"transitions": [
						{
							"aim": "v.is_holding_mk18empty && v.is_first_person"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						},
						{
							"aim": "v.is_first_person && v.mk18 && !v.s"
						},
						{
							"firing.delay.aimed": "v.is_first_person && v.is_holding_mk18 && q.all_animations_finished"
						}
					]
				},
				"firing.delay.aimed": {
					"on_entry": [ "v.t = q.life_time;" ],
					"animations": [
						{
							"mk18.first_person.aim": "v.is_first_person && v.mk18"
						}
					],
					"transitions": [
						{
							"firing.aimed": "(q.life_time - v.t) >= 0.01 && v.is_holding_mk18 && v.u"
						},
						{
							"aim": "v.is_first_person && (!v.is_holding_mk18 || v.is_holding_mk18empty) || !v.u"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						}
					]
				},
				"reloading": {
					"animations": [
						{
							"mk18.first_person.reload": "v.is_first_person && v.mk18"
						}
					],
					"transitions": [
						{
							"draw": "!v.mk18 || v.is_third_person"
						},
						{
							"first_person": "q.all_animations_finished && !v.r && v.mk18"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"sprinting": {
					"animations": [
						{
							"mk18.first_person.sprinting": "v.is_first_person && v.mk18"
						}
					],
					"transitions": [
						{
							"draw": "!v.mk18 || v.is_third_person"
						},
						{
							"aim": "v.is_first_person && v.mk18 && v.s"
						},
						{
							"first_person": "v.is_first_person && v.mk18 && !v.m"
						},
						{
							"reloading": "v.is_first_person && v.is_holding_mk18empty && v.r"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"aim": {
					"animations": [
						{
							"mk18.first_person.aim": "v.is_first_person && v.mk18"
						}
					],
					"transitions": [
						{
							"first_person": "v.is_first_person && v.mk18 && !v.s"
						},
						{
							"draw": "!v.mk18 || v.is_third_person"
						},
						{
							"firing.aimed": "v.is_first_person && v.is_holding_mk18 && v.u && v.s"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				}
			}
		},
		"controller.animation.mk18.third_person": {
			"initial_state": "draw",
			"states" : {
				"draw": {
					"animations": [
						{
							"mk18.third_person.draw": "v.mk18 && v.is_third_person"
						}
					],
					"transitions": [
						{
							"third_person": "q.all_animations_finished && v.mk18 && v.is_third_person"
						}
					]
				},
				"third_person" : {
					"animations" : [
						{ 
						    "mk18.third_person.holding": "v.is_third_person && v.mk18"
						}
					],
					"transitions" : [
						{
							"aim": "v.is_third_person && v.mk18 && v.s"
						},
						{
							"sprinting": "v.is_third_person && v.mk18 && v.m && !v.r"
						},
						{
							"reloading": "v.is_third_person && v.is_holding_mk18empty && v.r"
						},
						{
							"firing": "v.is_third_person && v.is_holding_mk18 && v.u"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"firing": {
					"animations": [
						{
							"mk18.third_person.fire": "v.is_third_person && v.is_holding_mk18"
						}
					],
					"transitions": [
						{
							"third_person": "v.is_holding_mk18empty && v.is_third_person"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						},
						{
							"third_person": "v.is_third_person && v.mk18 && v.s"
						},
						{
							"firing.delay": "v.is_third_person && v.is_holding_mk18 && q.all_animations_finished"
						}
					]
				},
				"firing.delay": {
					"on_entry": [ "v.t = q.life_time;" ],
					"animations": [
						{
							"mk18.third_person.holding": "v.is_third_person && v.mk18"
						}
					],
					"transitions": [
						{
							"firing": "(q.life_time - v.t) >= 0.01 && v.is_holding_mk18 && v.u"
						},
						{
							"third_person": "v.is_third_person && (!v.is_holding_mk18 || v.is_holding_mk18empty) || !v.u"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						}
					]
				},
				"firing.aimed": {
					"animations": [
						{
							"mk18.third_person.aim_fire": "v.is_third_person && v.is_holding_mk18"
						}
					],
					"transitions": [
						{
							"aim": "v.is_holding_mk18empty && v.is_third_person"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						},
						{
							"aim": "v.is_third_person && v.mk18 && !v.s"
						},
						{
							"firing.delay.aimed": "v.is_third_person && v.is_holding_mk18 && q.all_animations_finished"
						}
					]
				},
				"firing.delay.aimed": {
					"on_entry": [ "v.t = q.life_time;" ],
					"animations": [
						{
							"mk18.third_person.aim": "v.is_third_person && v.mk18"
						}
					],
					"transitions": [
						{
							"firing.aimed": "(q.life_time - v.t) >= 0.01 && v.is_holding_mk18 && v.u"
						},
						{
							"aim": "v.is_third_person && (!v.is_holding_mk18 || v.is_holding_mk18empty) || !v.u"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						}
					]
				},
				"reloading": {
					"animations": [
						{
							"mk18.third_person.reload": "v.is_third_person && v.mk18"
						}
					],
					"transitions": [
						{
							"draw": "!v.mk18 || v.is_first_person"
						},
						{
							"third_person": "q.all_animations_finished && !v.r && v.mk18"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"sprinting": {
					"animations": [
						{
							"mk18.third_person.sprinting": "v.is_third_person && v.mk18"
						}
					],
					"transitions": [
						{
							"draw": "!v.mk18 || v.is_first_person"
						},
						{
							"aim": "v.is_third_person && v.mk18 && v.s"
						},
						{
							"third_person": "v.is_third_person && v.mk18 && !v.m"
						},
						{
							"reloading": "v.is_third_person && v.is_holding_mk18empty && v.r"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"aim": {
					"animations": [
						{
							"mk18.third_person.aim": "v.is_third_person && v.mk18"
						}
					],
					"transitions": [
						{
							"third_person": "v.is_third_person && v.mk18 && !v.s"
						},
						{
							"draw": "!v.mk18 || v.is_first_person"
						},
						{
							"firing.aimed": "v.is_third_person && v.is_holding_mk18 && v.u && v.s"
						}
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}

